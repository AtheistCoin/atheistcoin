* { 
    gridline-color: #a3a3a3;
    outline: 0px;
    font-family: "Ubuntu",sans-serif;
}

#atheiststatus {
    color:#fff;
    background-color:#606262;
}

#splitter QListView {
    border:none;
    background-color:rgba(0, 0, 0, 99);
    border-radius:5px;
    color:#fff;
    padding:5px;
}

#splitter QLineEdit {
    border:none;
    background-color:rgba(0, 0, 0, 99);
    border-radius:5px;
    color:#fff;
    height:23px;
    font-size:12px;
}

#ChatWindowClass QTabWidget::pane {
    border:none;
    background-color:rgba(0, 0, 0, 99);
    border-bottom-left-radius:10px;
    border-bottom-right-radius:10px;
    border-top-right-radius:10px;
}

#ChatWindowClass QTabBar::tab {
    border:none;
    border-top-left-radius:10px;
    border-top-right-radius:10px;
    background-color:rgba(50, 50, 50, 99);
    color:#fff;
    margin-right:2px;
    padding:3px 6px;
}

#ChatWindowClass QTabBar::tab:selected {
    background-color:rgba(0, 0, 0, 99);
}

#ChatWindowClass QTabWidget QTextEdit {
    background-color:transparent;
    font-family: "Ubuntu Mono", monospace;
    font-weight:normal;
    color:#fff;
    font-size:12px;
}

QDialog,
QMainWindow #believe {
    color: #fff;
    background-image: url(:/images/mainbg);
    background-position: top center;
    background-repeat: no-repeat;
    background-color: #606262;
    border: none;
}

#verticalLayout_2 QFrame {
    color:#757575; 
}

#labelTransactionsStatus,
#labelWalletStatus {
    color: #79B3E4;
}

#messagesWidget {
    background-color:transparent;
    color:#fff;
    border-radius:5px;
}

#listTransactions,
#scrollAreaWidgetContents,
#SendCoinsDialog {
    color:#fff;
}

#labelAlerts { 
    background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0 #F0D0A0, stop:1 #F8D488); 
    color:#000000; 
}

#OverviewPage #frame{
    border:1px solid #747474;
    border-radius: 8px;
}

#TransactionDescDialog #detailText,
#OverviewPage #listTransactions{
    color:#fff;
    background-color:transparent;
}
#SendCoinsEntry QValidatedLineEdit{
    color:#000;
}
#SendCoinsDialog #scrollAreaWidgetContents,
#SendCoinsEntry QToolButton,
#SendCoinsEntry QToolButton::hover{
    background-color:transparent;
}

QDialog QLineEdit{
    background-color: #fff;
    color: #000;
}

QDialog{
    background-color: #606262;
}

#SendCoinsDialog,
#TransactionView{
    background-color: transparent;
}

#RPCConsole tab_info{
    background-color: #606262;
    color: #000;
}
#RPCConsole QLineEdit{
    background-color: #fff;
    color: #000;
}
#SignVerifyMessageDialog QPlainTextEdit{
    background-color: #fff;
    color: #000;
    border-radius: 4px;
}

appMenuBar {
    color:#fff;
    background-color:#275079; 
    border:0px; 
}

QHeaderView {
    background-color: #336699;
	border: none;
}

QHeaderView::section {
    color: #fff;
    padding: 5px;
    border:none;
    background-color: #336699;
    border:none;
}

QLabel {
    color: #fff;
}

QMenuBar {
    color:#fff;
    background-color:#275079; 
    border:0px; 
}

QMenuBar::item {
    color:#fff;
    background-color:#275079; 
    margin-left:10px;
    margin-top:2px;
    margin-right:2px;
    margin-bottom:2px;
    border:0px;
}

QProgressBar {
    color: #fff;
    border-top-style: inset;
    border:1px solid #a3a3a3;
    text-align: center;
    border-radius:4px;
    padding:1px;
    background-color: #336699;
}

QProgressBar::chunk {
    color: #336699;
    background-color: rgba(255,255,255,50);
    font-weight: normal;    
    width: 5px;
}

QScrollArea{
    background-color:transparent;
}

QStatusBar::item {
    border:none;
}

QTableView {
    color: #000;
    background-color:transparent;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
}

QTextEdit {
    color: #fff;
    border:none;
    border-radius:2px;
}

QToolBar {
    margin:0px;
    padding:0px;
    spacing:15px;
    border: none; 
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #275079, stop: 0.4 #336699, stop: 0.5 #336699, stop: 1.0 #336699); 
}
QToolBar QToolButton{
    min-height: 60px;
    width:100%;
    color: #fff;
}

QPushButton,
QPushButton::focus{	
    color: #fff;
    border:none;
}

QToolButton {
    background-color: transparent;
    border:none;
    color: #fff;

}

QToolButton::hover {
    border:none;
    background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 transparent, stop: 0.4 #3c74ac, stop: 0.5 #3c74ac, stop: 1.0 transparent); 
}

QToolButton::checked {
    border:none;
    background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 transparent, stop: 0.4 #2d5a88, stop: 0.5 #2d5a88, stop: 1.0 transparent);
}

QToolTip {
    color: #fff;
    background-color: #9b9b9b;
    border: 1px solid #a3a3a3;
}

QWindowsStyle QProgressBar,
QWindowsXPStyle QProgressBar {
    background-color: #e8e8e8; 
    border: 1px solid #a3a3a3; 
    border-radius: 5px; 
    padding: 1px; 
    text-align: center;
}
QWindowsStyle QProgressBar::chunk,
QWindowsXPStyle QProgressBar::chunk {
    color:#fff;
    background-color:gray;
    border-radius:5px;
    margin: 0px;
}